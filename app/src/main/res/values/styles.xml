<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomChipStyle_filter" parent = "Widget.MaterialComponents.Chip.Filter">
        <item name="chipBackgroundColor">@color/filter_chip_bg_color</item>
        <item name="android:textColor">@color/filter_chip_text_color</item>
        <item name="checkedIconTint">@color/white</item>
    </style>

    <style name="CustomChipStyle_areaChoice" parent = "Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor" >@color/choice_chip_bg_color</item>
        <item name="android:textColor">@color/choice_chip_text_color</item>
        <item name="checkedIconTint">@color/white</item>
        <item name="fontFamily">@font/spoqa_han_sans_neo2</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="CustomHeartBookMark">
        <item name="android:drawable">@drawable/heart_selector</item>
    </style>

</resources>



